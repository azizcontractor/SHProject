<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" maxHeight="400.0" maxWidth="300.0" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shproject.SystemStateController">
   <children>
      <Label fx:id="systemStatelbl" alignment="CENTER" layoutX="51.0" layoutY="64.0" text="System State">
         <font>
            <Font name="Lucida Bright Regular" size="32.0" />
         </font>
      </Label>
      <Button fx:id="homebtn" layoutX="77.0" layoutY="187.0" mnemonicParsing="false" onAction="#btnHomeClicked" prefHeight="50.0" prefWidth="150.0" text="HOME">
         <font>
            <Font name="Lucida Bright Regular" size="25.0" />
         </font>
      </Button>
      <Button fx:id="awaybtn" layoutX="77.0" layoutY="257.0" mnemonicParsing="false" onAction="#btnAwayClicked" prefHeight="50.0" prefWidth="150.0" text="AWAY">
         <font>
            <Font name="Lucida Bright Regular" size="25.0" />
         </font>
      </Button>
      <Button fx:id="travelbtn" layoutX="77.0" layoutY="327.0" maxHeight="50.0" maxWidth="200.0" mnemonicParsing="false" onAction="#btnTravelClicked" prefHeight="50.0" prefWidth="150.0" text="TRAVEL">
         <font>
            <Font name="Lucida Bright Regular" size="25.0" />
         </font>
      </Button>
      <Label fx:id="lblcurrStateEmpty" alignment="CENTER" layoutX="163.0" layoutY="109.0" prefHeight="30.0" prefWidth="100.0" text="?">
         <font>
            <Font name="Lucida Bright Regular" size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblstate" alignment="CENTER" layoutX="29.0" layoutY="109.0" prefHeight="30.0" prefWidth="126.0" text="Current State:">
         <font>
            <Font name="Lucida Bright Regular" size="18.0" />
         </font>
      </Label>
      <Button fx:id="mainbtn" layoutX="240.0" layoutY="14.0" mnemonicParsing="false" onAction="#goMainPage" prefHeight="50.0" prefWidth="46.0" />
      <ImageView fitHeight="50.0" fitWidth="46.0" layoutX="240.0" layoutY="14.0">
         <image>
            <Image url="@../house.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
