<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shproject.TemperatureController">
   <children>
      <Label layoutX="49.0" layoutY="82.0" text="Temperature">
         <font>
            <Font name="Lucida Bright Regular" size="32.0" />
         </font>
      </Label>
      <Button fx:id="setbtn" layoutX="117.0" layoutY="276.0" mnemonicParsing="false" onAction="#setTemp" prefHeight="48.0" prefWidth="66.0" text="SET">
         <font>
            <Font name="Lucida Bright Regular" size="21.0" />
         </font>
      </Button>
      <Circle fill="#fffdfd" layoutX="230.0" layoutY="154.0" radius="8.0" stroke="BLACK" strokeType="INSIDE" />
      <Button fx:id="backbtn" layoutX="12.0" layoutY="355.0" mnemonicParsing="false" onAction="#goBack" prefHeight="16.0" prefWidth="66.0" text="Back" />
      <TextField fx:id="temptext" alignment="CENTER" layoutX="71.0" layoutY="145.0" maxWidth="145.0" minHeight="100.0" minWidth="130.0" onAction="#setTemptext" prefHeight="111.0" prefWidth="140.0">
         <font>
            <Font size="55.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
